project LCD is
   for Target use "arm-eabi";
   for Main use ("lcd.adb");
   for Source_Files use ("lcd.adb");
   for Exec_Dir use ".";
   for Object_Dir use ".build";
   package Builder is
      for Default_Switches ("ada") use
        ("-g",
         "-O0",
         "--RTS=../stm32f429i-disco-bsp");
   end Builder;
   package Linker is
      for Default_Switches ("ada") use
        (
         "-Wl,-Map," & Project'Project_Dir & Project'Name & ".map"
         );
   end Linker;
end LCD;
